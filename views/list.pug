doctype html
html
  head
    meta(charset='utf-8')
  body
    .output
    script.
      fetch("/students/list")
      .then(resp => {
      if (resp.status == 200) {
      resp.json()
      .then(ret => {
      if (ret.status == "OK")
      document.querySelector("div.output").innerHTML = JSON.stringify(ret.data);
      });
      }
      });
